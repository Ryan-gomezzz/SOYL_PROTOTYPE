{
  "version": 2,
  "builds": [
    {
      "src": "packages/app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/app/$1"
    }
  ],
  "env": {
    "VITE_API_BASE": "https://8zaa4i3xma.execute-api.ap-south-1.amazonaws.com/prod/"
  }
}
